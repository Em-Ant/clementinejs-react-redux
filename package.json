{
  "name": "options_inc_webapp",
  "version": "1.0.1",
  "description": "A transportation routing solution for Options Inc",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Em-Ant/fcc-options-app.git"
  },
  "homepage": "https://options-inc.herokuapp.com",
  "bugs": "https://github.com/Em-Ant/fcc-options-app/issues",
  "keywords": [
    "MongoDB",
    "Mongoose",
    "Full stack",
    "Express",
    "Node",
    "Javascript",
    "React",
    "Redux",
    "Webpack"
  ],
  "scripts": {
    "start": "node server.js",
    "serve": "babel-node server.js",
    "clean": "rm -rf dist && mkdir -p dist/client",
    "wpack": "webpack -p && cp -R ./client/public ./dist/client/public",
    "bcli": "babel app --out-dir ./dist/app",
    "build": "npm run clean && npm run wpack && npm run bcli"
  },
  "author": "Emanuele Antonelli <joshua999x@gmail.com> (http://emant.altervista.org/)",
  "contributors": [
    "Andre Alonzo <aalonzo@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "dotenv": "^1.2.0",
    "express": "^4.12.4",
    "express-session": "^1.11.3",
    "mongoose": "^4.1.0",
    "passport": "^0.3.0",
    "passport-twitter": "^1.0.4",
    "react-router": "^2.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "redux": "^3.3.0",
    "redux-thunk": "^1.0.3",
    "resolve-url": "^0.2.1",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12"
  }
}
